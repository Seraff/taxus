{
  "name": "taxus",
  "version": "0.2.1-alpha",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Seraff/taxus"
  },
  "scripts": {
    "start": "electron .",
    "test": "./node_modules/mocha/bin/mocha test/*_test.js",
    "start-no-gpu": "electron . --disable-gpu --force-cpu-draw",
    "standard": "standard",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w",
    "dist-lin": "electron-builder -l",
    "docs": "cd docs; make html; cd ../"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5",
    "mocha": "^9.2.0",
    "spectron": "^11.0.0",
    "spectron-menu-addon": "^0.4.1",
    "standard": "^16.0.4",
    "xml2js": "0.4.19"
  },
  "keywords": [
    "phylogeny",
    "bioinformatics"
  ],
  "build": {
    "appId": "com.kolisko-lab.taxus",
    "fileAssociations": [
      {
        "name": "Tree file",
        "ext": "tre",
        "role": "Editor"
      },
      {
        "name": "Tree file",
        "ext": "tree",
        "role": "Editor"
      },
      {
        "name": "Tree file",
        "ext": "nexus",
        "role": "Editor"
      },
      {
        "name": "Tree file",
        "ext": "nex",
        "role": "Editor"
      },
      {
        "name": "Tree file",
        "ext": "nxs",
        "role": "Editor"
      },
      {
        "name": "Tree file",
        "ext": "newick",
        "role": "Editor"
      }
    ]
  },
  "author": "Serafim Nenarokov",
  "license": "ISC",
  "dependencies": {
    "a-color-picker": "^1.2.1",
    "ansi-regex": "^6.0.1",
    "bio-parsers": "^6.0.4",
    "electron-in-page-search": "^1.3.2",
    "electron-localshortcut": "^3.2.1",
    "electron-progressbar": "^2.0.0",
    "electron-unhandled": "^3.0.2",
    "file-dialog": "0.0.7",
    "jquery": "^3.5.1",
    "mousetrap": "^1.6.5",
    "pako": "^1.0.11",
    "path-data-polyfill": "^1.0.3",
    "save-svg-as-png": "^1.4.17",
    "split.js": "^1.5.11",
    "string-width": "^5.1.0",
    "underscore": "^1.13.1",
    "ve-range-utils": "^2.5.8",
    "ve-sequence-utils": "^3.3.54"
  }
}
